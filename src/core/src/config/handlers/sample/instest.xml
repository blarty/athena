<!DOCTYPE chi >

<chi>
	<name>athenatest</name>
    <context key="SYS1">
    <params>
	    <param name="code" validation="NOTNULL|REQUIRED"/>
		<param name="description" validation="NOTNULL|REQUIRED"/>
		<param name="quantity" validation="NOTNULL|REQUIRED"/>
    </params>
    <command type="replace">
		<command-text>
	    INSERT into AthenaTest(CODE,DESCRIPTION, QUANTITY)  VALUES('${code}', '${description}',${quantity})
	    </command-text>
    </command>
    </context>
</chi>

