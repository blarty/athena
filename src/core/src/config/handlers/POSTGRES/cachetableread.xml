<!DOCTYPE chi >

<chi>
	<name>cacheread</name>
    <context key="SYS1">
    <params>
		<param name="name" validation="NOTNULL|REQUIRED"/>
    </params>
    <command type="replace" useCache="true">
	    <command-text>
	    	SELECT * from cachetest where name='${name}'
	    </command-text>
    </command>
    </context>
</chi>

